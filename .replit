[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run analyze"

[deployment]
run = ["sh", "-c", "NODE_ENV=development PORT=80 INTERNAL_PORT=3001 PGPORT=5432 node --import tsx src/server/server.ts"]
deploymentTarget = "cloudrun"

[[ports]]
localPort = 3001
externalPort = 3000

[[ports]]
localPort = 5173
externalPort = 5000
exposeLocalhost = true
